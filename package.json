  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HLPFLCG/whitelabellinkinbio.git"
  },
  "homepage": "https://hlpfl.org",
  "bugs": {
    "url": "https://github.com/HLPFLCG/whitelabellinkinbio/issues"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "html-validate": "^8.0.0",
    "stylelint": "^15.0.0",
    "stylelint-config-standard": "^33.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
